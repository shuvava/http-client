        <!-- Authors contain text that appears directly on the gallery -->
        <authors>shuvava</authors>
        <owners>shuvava</owners>
        <licenseUrl>https://raw.githubusercontent.com/shuvava/http-client/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/shuvava/http-client</projectUrl>

        <requireLicenseAcceptance>false</requireLicenseAcceptance>

        <description>HTTP Client (REST, Stream)</description>


        <!-- Tags appear in the gallery and can be used for tag searches -->
