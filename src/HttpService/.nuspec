            <dependency id="RestServiceClient.Abstractions" version="1.0.0" />
        </dependencies>
    </metadata>
    <files>
        <file src="HttpService.dll" target="lib\netstandard2.0" />
        <file src="HttpService.pdb" target="lib\netstandard2.0" />
